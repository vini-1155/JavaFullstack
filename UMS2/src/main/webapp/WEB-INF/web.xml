<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>UMS2</display-name>
  <welcome-file-list>
    <welcome-file>addstudent.html</welcome-file>
    <welcome-file>addsubject.html</welcome-file>
  </welcome-file-list>
  <servlet>
        <servlet-name>one</servlet-name>
        <servlet-class>ANANTHA.CONTROLLER.AddStudentController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>one</servlet-name>
        <url-pattern>/addstudent</url-pattern>
    </servlet-mapping>

    <!-- Servlet Mapping for AddSubjectServlet -->
    <servlet>
        <servlet-name>two</servlet-name>
        <servlet-class>ANANTHA.CONTROLLER.AddSubjectController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>two</servlet-name>
        <url-pattern>/addsubject</url-pattern>
    </servlet-mapping>
</web-app>